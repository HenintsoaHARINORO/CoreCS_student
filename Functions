;; The first three lines of this file were inserted by DrRacket. They record metadata
;; about the language level of this file in a form that our tools can easily process.
#reader(lib "htdp-beginner-reader.ss" "lang")((modname Functions) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f () #f)))
(require 2htdp/image)
(define (FunctionName  d c)
   (circle d "solid" c))

(overlay (FunctionName 40 "purple")
         (FunctionName 50 "red")
         (FunctionName 60 "yellow"))

(define (Function  c)
   (circle 40 "solid" c))

(Function (string-append "re" "d"))
(Function "red")
(circle 40 "solid" "red")

